algoritmo "Primeiro"
// Disciplina   : [Linguagem e Lógica de Programação] 
// Professor   : Antonio Carlos Nicolodi 
var
  msg: caractere
  recado: caractere
inicio
  msg <- "Olá, Mundo!"
  recado <- "Hoje é seu aniversário"
         Escreval(msg)
         Escreval("Me livrei da maldição..")
         Escreval("Parabêns ", recado)
fimalgoritmo